{
    "schemaVersion": 1,
    "id": "improvedvanilla",
    "version": "${version}",
    "name": "Improved Vanilla",
    "description": "small changes/additions to make the gameplay a little bit more pleasant/enjoyable",
    "authors": [
        "Buecher_wurm"
    ],
    "contact": {
        "sources": "https://github.com/tristankechlo/ImprovedVanilla",
        "issues": "https://github.com/tristankechlo/ImprovedVanilla/issues"
    },
    "license": "CC BY-NC-ND 4.0",
    "icon": "pack.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.tristankechlo.improvedvanilla.FabricImprovedVanilla"
        ]
    },
    "mixins": [
        "improvedvanilla.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${fabric_loader_min}",
        "fabric-api": "*",
        "minecraft": ">=${fabric_range_min_inclusive} <${fabric_range_max_exclusive}",
        "java": ">=${java_version}"
    },
    "custom": {
        "modmenu": {
            "links": {
                "modmenu.discord": "https://discord.gg/bhUaWhq",
                "modmenu.curseforge": "https://www.curseforge.com/minecraft/mc-mods/improved-vanilla",
                "modmenu.modrinth": "https://modrinth.com/mod/improved-vanilla",
                "modmenu.wiki": "https://github.com/tristankechlo/ImprovedVanilla/wiki"
            }
        }
    }
}